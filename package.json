{
  "name": "sitetest-source",
  "version": "1.0.0",
  "description": "DroidKaigi Site",
  "scripts": {
    "build": "npm lint && gatsby build",
    "build-release": "npm lint && gatsby build --prefix-paths",
    "build-preview": "npm lint && scripts/build-preview 2018",
    "build-dummy": "npm lint && scripts/build-dummy",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "lint": "eslint src 'gatsby-*'",
    "format": "eslint --fix src 'gatsby-*'",
    "test": ":"
  },
  "dependencies": {
    "axios": "0.16.2",
    "babel-eslint": "7.2.3",
    "eslint": "4.3.0",
    "eslint-config-standard": "10.2.1",
    "eslint-config-standard-react": "5.0.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-node": "5.1.1",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-react": "7.1.0",
    "eslint-plugin-standard": "3.0.1",
    "gatsby": "1.3.2",
    "gatsby-link": "1.3.2",
    "gatsby-plugin-react-helmet": "1.0.1"
